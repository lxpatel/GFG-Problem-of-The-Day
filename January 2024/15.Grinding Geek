// Grinding Geek
// January 15, 2024
// C++ Code

class Solution{
    public:
   int max_courses(int n, int t, vector<int> &cost)
    {
        //Code Here
        int cnt =0;
        int p = t;
        for(int i =0; i<n; i++){
            if(p>= cost[i]){
                p -= ceil(0.1*cost[i]);
                cnt++;
            }
        }
        return cnt;
    }
};
